{namespace ImageEditorCropHandles}

/**
 * Crop Handles
 *
 * @param key
 */
{template .render}
	<div id="{$key}" class="hide" style="width: 200px; height: 200px; position: absolute; top: 100px; left: 100px; background-color: rgba(100, 0, 100, 0.5);">
		<div class="resize-handle" style="width: 20px; height: 20px; background-color: #F0F; position: absolute; bottom: 0; right: 0;"></div>
	</div>
{/template}