{namespace ImageEditorBrightnessComponent}

/**
 * Image Editor Brightness Component
 *
 * @param key
 * @param requestPreview
 */
{template .render}
	<div id="{$key}">
		{call Slider.render}
			{param events: [
				'valueChanged': $requestPreview
			] /}
			{param key: 'slider' /}
			{param value: 50 /}
		{/call}
	</div>
{/template}